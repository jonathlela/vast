<?xml version="1.0" encoding="big5"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.10"
	Name="VASTsim"
	RootNamespace="VASTsim"
	SccProjectName=""
	SccLocalPath="">
	<Platforms>

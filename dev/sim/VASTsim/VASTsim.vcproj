				BufferSecurityCheck="TRUE"
				UsePrecompiledHeader="2"
				ProgramDataBaseFileName="$(IntDir)/$(ProjectName).pdb"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>

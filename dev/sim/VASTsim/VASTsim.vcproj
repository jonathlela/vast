copy &quot;$(OutDir)\$(TargetName).pdb&quot; &quot;$(ProjectDir)..\..\bin&quot;
copy &quot;$(OutDir)\$(TargetFileName)&quot; &quot;$(ProjectDir)..\..\bin&quot;
copy &quot;$(OutDir)\$(TargetName).pdb&quot; &quot;$(ProjectDir)..\..\bin&quot;
copy &quot;$(OutDir)\$(TargetName).lib&quot; &quot;$(ProjectDir)..\..\bin&quot;
"/>

				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				OutputFile="$(OutDir)$(ProjectName).dll"
				LinkIncremental="0"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(SolutionDir)\..\ACE_wrappers\lib.vc7"
				ProgramDatabaseFile="$(OutDir)/$(TargetName).pdb"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\..\bin/VASTsim.tlb"
				HeaderFileName=""/>

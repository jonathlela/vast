				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				OutputFile="$(OutDir)$(ProjectName)d.dll"
				LinkIncremental="0"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(SolutionDir)\..\ACE_wrappers\lib.vc7"
